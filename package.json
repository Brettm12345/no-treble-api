{
  "name": "no-trebble-api",
  "scripts": {
    "start": "ts-node-dev --no-notify --respawn --transpileOnly ./src",
    "debug": "dotenv -- nodemon -e ts,graphql -x ts-node --inspect src/index.ts",
    "generate": "prisma generate && npx nexus-prisma-generate --client ./src/generated/prisma-client --output ./src/generated/nexus-prisma",
    "build": "rimraf dist && tsc",
    "lint": "eslint --ext .ts ./src ./tests",
    "lint:fix": "yarn -s lint --fix"
  },
  "dependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/jsonwebtoken": "^8.3.5",
    "@types/ramda": "^0.26.33",
    "apollo-server": "^2.9.6",
    "bcrypt": "^3.0.6",
    "bcryptjs": "2.4.3",
    "fp-ts": "^2.1.0",
    "graphql": "^14.5.8",
    "jsonwebtoken": "^9.0.0",
    "nexus": "^0.11.7",
    "nexus-prisma": "0.3.7",
    "prisma-client-lib": "^1.34.10",
    "ramda": "^0.26.1"
  },
  "resolutions": {
    "graphql": "14.5.8"
  },
  "devDependencies": {
    "@types/bcryptjs": "2.4.2",
    "@types/node": "12.11.1",
    "@typescript-eslint/eslint-plugin": "^2.4.0",
    "@typescript-eslint/parser": "^2.4.0",
    "dotenv-cli": "3.0.0",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-fp": "^2.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-sort-keys-fix": "^1.0.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-unicorn": "^12.1.0",
    "nodemon": "1.19.4",
    "prettier": "^1.18.2",
    "prisma": "^1.34.10",
    "rimraf": "3.0.0",
    "ts-node": "7.0.1",
    "ts-node-dev": "^1.0.0-pre.43",
    "typescript": "3.6.4"
  }
}
